{
	"name": "Limestone light 0.8",
	"type": "dark",
	"colors": {
		"editor.background":                   "#fff9e6",
		"editor.foreground":                   "#595959",
		"editorCursor.foreground":             "#595959",
		"editorLineNumber.foreground":         "#a9a59d",
		"editorLineNumber.activeForeground":   "#595959",
		"editorIndentGuide.background":        "#d5cebc",
		"editorIndentGuide.activeBackground":  "#a9a59d",
		"editor.selectionBackground":          "#d5cebc",
		"editor.inactiveSelectionBackground":  "#f1ead8",
		"editor.selectionHighlightBackground": "#fff9e6",
		"editor.selectionHighlightBorder":     "#d5cebc"
	},
	"tokenColors": [
		{
			"scope": "comment",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#95938c"
			}
		},
		{
			"scope": "string",
			"settings": {
				"fontStyle": "normal",
				"foreground": "#5464b4"
			}
		},
		{
			"scope": "string.quoted.docstring",
			"settings": {
				"fontStyle": "normal",
				"foreground": "#95938c"
			}
		},
		{
			"name": "special elements in doc comments",
			"scope": "storage.type.class.jsdoc",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#807e7a"
			}
		},
		{
			"name": "string interpolation and escaping",
			"scope": [
				"string.constant.character.escape",
				"constant.character.format",
				"constant.character.escape",
				"punctuation.section.embedded",
				"punctuation.definition.template-expression"
			],
			"settings": {
				"fontStyle": "normal",
				"foreground": "#373737"
			}
		},
		{
			"name": "code embedded in string interpolation",
			"scope": [
				"meta.embedded"
			],
			"settings": {
				"fontStyle": "normal",
				"foreground": "#595959"
			}
		},
		{
			"name": "constant",
			"scope": [
				"constant",
				"variable.other.constant"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#73726f"
			}
		},
		{
			"scope": "constant.numeric",
			"settings": {
				"fontStyle": "normal",
				"foreground": "#366cf0"
			}
		},
		{
			"scope": "constant.language",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#366cf0"
			}
		},
		{
			"name": "keywords (including function and class defs)",
			"scope": [
				"keyword.control",
				"storage.type.function",
				"storage.type.class"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#55704b"
			}
		},
		{
			"name": "imports",
			"scope": [
				"keyword.control.import",
				"keyword.other.import"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#55704b"
			}
		},
		{
			"scope": "keyword.operator",
			"settings": {
				"fontStyle": "normal",
				"foreground": "#2a8318"
			}
		},
		{
			"name": "types and type modifiers",
			"scope": "storage",
			"settings": {
				"fontStyle": "bold italic",
				"foreground": "#95938c"
			}
		},
		{
			"name": "keyword argument name in function call",
			"scope": "variable.parameter.function-call",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#73726f"
			}
		},
		{
			"name": "reserved variables like this, super, self",
			"scope": "variable.language",
			"settings": {
				"fontStyle": "bold italic",
				"foreground": "#95938c"
			}
		},
		{
			"name": "built-ins, things from library/framework",
			"scope": "support",
			"settings": {
				"fontStyle": "normal",
				"foreground": "#457337"
			}
		},
		{
			"scope": "support.type.exception",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#373737"
			}
		},
		{
			"name": "Function decorator and similar elements",
			"scope": [
				"entity.name.function.decorator",
				"support.type.python"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#457337"
			}
		},
		{
			"name": "Name of the defined class",
			"scope": [
				"entity.name.class",
				"entity.name.type.class"
			],
			"settings": {
				"fontStyle": "bold underline",
				"foreground": "#424242"
			}
		},
		{
			"name": "Name of the defined function",
			"scope": [
				"entity.name.function",
				"entity.name.function.declaration"
			],
			"settings": {
				"fontStyle": "underline",
				"foreground": "#424242"
			}
		},
		{
			"name": "function calls (works only for some languages)",
			"scope": "meta.function-call.generic",
			"settings": {
				"fontStyle": "normal",
				"foreground": "#595959"
			}
		},
		{
			"name": "function calls and defs use the same scope in JS",
			"scope": "entity.name.function.js",
			"settings": {
				"fontStyle": "normal",
				"foreground": "#595959"
			}
		},
		{
			"name": "ordinary variables (should by default use basic styling)",
			"scope": "variable",
			"settings": {
				"foreground": "#595959"
			}
		},
		{
			"scope": "variable.parameter",
			"settings": {
				"fontStyle": "normal",
				"foreground": "#595959"
			}
		},
		{
			"name": "dots, parens etc. (token separators)",
			"scope": [
				"punctuation.definition.block",
				"punctuation.definition.parameters",
				"punctuation.definition.arguments",
				"punctuation.definition.list",
				"punctuation.definition.dict",
				"punctuation.definition.binding-pattern",
				"punctuation.parenthesis",
				"punctuation.separator",
				"punctuation.accessor",
				"punctuation.section"
			],
			"settings": {
				"fontStyle": "normal",
				"foreground": "#373737"
			}
		}
	]
}