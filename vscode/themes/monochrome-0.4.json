{
	"name": "Monochrome 0.4",
	"type": "dark",
	"colors": {
		"editor.background":                   "#262626",
		"editor.foreground":                   "#b9b9b9",
		"editorCursor.foreground":             "#b9b9b9",
		"editorLineNumber.foreground":         "#6a6a6a",
		"editorLineNumber.activeForeground":   "#b9b9b9",
		"editorIndentGuide.background":        "#474747",
		"editorIndentGuide.activeBackground":  "#6a6a6a",
		"editor.selectionBackground":          "#474747",
		"editor.inactiveSelectionBackground":  "#303030",
		"editor.selectionHighlightBackground": "#262626",
		"editor.selectionHighlightBorder":     "#474747"
	},
	"tokenColors": [
		{
			"scope": "comment",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#6a6a6a"
			}
		},
		{
			"scope": "string",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#7d7d7d"
			}
		},
		{
			"name": "docstrings and other multiline strings",
			"scope": [
				"string.unquoted",
				"string.quoted.docstring.multi"
			],
			"settings": {
				"fontStyle": "normal",
				"foreground": "#6a6a6a"
			}
		},
		{
			"name": "special elements in doc comments",
			"scope": "storage.type.class.jsdoc",
			"settings": {
				"fontStyle": "bold italic",
				"foreground": "#7d7d7d"
			}
		},
		{
			"name": "string interpolation",
			"scope": [
				"string.constant.character.escape",
				"constant.character.format",
				"constant.character.escape",
				"punctuation.section.embedded",
				"punctuation.definition.template-expression.begin",
				"punctuation.definition.template-expression.end",
				"storage.type.string"
			],
			"settings": {
				"fontStyle": "normal",
				"foreground": "#e2e2e2"
			}
		},
		{
			"scope": "constant",
			"settings": {
				"fontStyle": "normal",
				"foreground": "#b9b9b9"
			}
		},
		{
			"scope": "constant.numeric",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#b9b9b9"
			}
		},
		{
			"scope": "constant.language",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#b9b9b9"
			}
		},
		{
			"name": "keywords (including function and class defs)",
			"scope": [
				"keyword.control",
				"storage.type.function",
				"storage.type.class"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#d4d4d4"
			}
		},
		{
			"name": "imports",
			"scope": [
				"keyword.control.import",
				"keyword.other.import"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#d4d4d4"
			}
		},
		{
			"scope": "keyword.operator",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#b9b9b9"
			}
		},
		{
			"name": "types and type modifiers",
			"scope": "storage",
			"settings": {
				"fontStyle": "bold italic",
				"foreground": "#7d7d7d"
			}
		},
		{
			"name": "const, final, abstract etc.",
			"scope": "storage.modifier",
			"settings": {
				"fontStyle": "bold italic",
				"foreground": "#6a6a6a"
			}
		},
		{
			"name": "reserved variables like this, super, self",
			"scope": "variable.language",
			"settings": {
				"fontStyle": "bold italic",
				"foreground": "#7d7d7d"
			}
		},
		{
			"name": "built-ins, things from library/framework",
			"scope": "support",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#b9b9b9"
			}
		},
		{
			"scope": "support.type.exception",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#e2e2e2"
			}
		},
		{
			"scope": "entity.name.function.decorator",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#b9b9b9"
			}
		},
		{
			"name": "Name of the defined class",
			"scope": [
				"entity.name.class",
				"entity.name.type.class"
			],
			"settings": {
				"fontStyle": "bold underline",
				"foreground": "#d4d4d4"
			}
		},
		{
			"name": "Name of the defined entity",
			"scope": "entity.name",
			"settings": {
				"fontStyle": "underline",
				"foreground": "#d4d4d4"
			}
		},
		{
			"scope": "meta.function-call",
			"settings": {
				"fontStyle": "normal",
				"foreground": "#b9b9b9"
			}
		},
		{
			"name": "ordinary variables (should by default use basic styling)",
			"scope": "variable",
			"settings": {
				"foreground": "#b9b9b9"
			}
		},
		{
			"scope": "variable.parameter",
			"settings": {
				"fontStyle": "normal",
				"foreground": "#b9b9b9"
			}
		},
		{
			"name": "Almost every variable in JS is in this scope",
			"scope": "variable.other",
			"settings": {
				"foreground": "#b9b9b9"
			}
		},
		{
			"name": "dots, parens etc. (token separators)",
			"scope": [
				"punctuation.definition.block",
				"punctuation.definition.parameters",
				"punctuation.definition.arguments",
				"punctuation.definition.list",
				"punctuation.definition.dict",
				"punctuation.definition.binding-pattern",
				"punctuation.section.block",
				"punctuation.section.function",
				"punctuation.section.method",
				"punctuation.section.class",
				"punctuation.section.array",
				"punctuation.separator",
				"punctuation.accessor"
			],
			"settings": {
				"fontStyle": "normal",
				"foreground": "#e2e2e2"
			}
		}
	]
}