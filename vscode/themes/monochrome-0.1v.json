{
	"name": "Monochrome 0.1v",
	"type": "dark",
	"colors": {
		"editor.background":                   "#262626",
		"editor.foreground":                   "#b9b9b9",
		"editorCursor.foreground":             "#b9b9b9",
		"editorLineNumber.foreground":         "#797979",
		"editorLineNumber.activeForeground":   "#b9b9b9",
		"editorIndentGuide.background":        "#474747",
		"editorIndentGuide.activeBackground":  "#797979",
		"editor.selectionBackground":          "#474747",
		"editor.inactiveSelectionBackground":  "#323232",
		"editor.selectionHighlightBackground": "#262626",
		"editor.selectionHighlightBorder":     "#474747"
	},
	"tokenColors": [
		{
			"scope": "comment",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#797979"
			}
		},
		{
			"name": "keywords (including function and class defs)",
			"scope": [
				"keyword.control",
				"storage.type.function",
				"storage.type.class"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#cdcdcd"
			}
		},
		{
			"scope": "keyword.operator",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#cdcdcd"
			}
		},
		{
			"name": "imports",
			"scope": [
				"keyword.control.import",
				"keyword.other.import"
			],
			"settings": {
				"fontStyle": "underline",
				"foreground": "#f1f1f1"
			}
		},
		{
			"scope": "string",
			"settings": {
				"foreground": "#989898"
			}
		},
		{
			"name": "docstrings and other multiline strings",
			"scope": [
				"string.unquoted",
				"string.quoted.docstring.multi"
			],
			"settings": {
				"foreground": "#797979"
			}
		},
		{
			"scope": "constant",
			"settings": {
				"foreground": "#cdcdcd"
			}
		},
		{
			"scope": "constant.numeric",
			"settings": {
				"foreground": "#f1f1f1"
			}
		},
		{
			"name": "string interpolation",
			"scope": [
				"string.constant.character.escape",
				"constant.character.format",
				"constant.character.escape",
				"punctuation.section.embedded",
				"punctuation.definition.template-expression.begin",
				"punctuation.definition.template-expression.end",
				"storage.type.string"
			],
			"settings": {
				"foreground": "#cdcdcd"
			}
		},
		{
			"scope": "storage",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#797979"
			}
		},
		{
			"name": "const, final, abstract etc.",
			"scope": "storage.modifier",
			"settings": {
				"foreground": "#989898"
			}
		},
		{
			"name": "Name of the defined entity",
			"scope": "entity.name",
			"settings": {
				"fontStyle": "bold underline",
				"foreground": "#f1f1f1"
			}
		},
		{
			"scope": "entity.name.function.decorator",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#989898"
			}
		},
		{
			"scope": "variable",
			"settings": {
				"foreground": "#b9b9b9"
			}
		},
		{
			"name": "Almost every variable in JS is in this scope",
			"scope": "variable.other",
			"settings": {
				"foreground": "#b9b9b9"
			}
		},
		{
			"name": "reserved language variables like this, super, self",
			"scope": "variable.language",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#989898"
			}
		},
		{
			"name": "stuff from library/framework",
			"scope": "support",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#989898"
			}
		},
		{
			"scope": "support.type.exception",
			"settings": {
				"fontStyle": "underline",
				"foreground": "#f1f1f1"
			}
		},
		{
			"name": "dots, parens etc. (token separators)",
			"scope": [
				"punctuation.definition.block",
				"punctuation.definition.parameters",
				"punctuation.definition.arguments",
				"punctuation.definition.list",
				"punctuation.definition.dict",
				"punctuation.definition.binding-pattern",
				"punctuation.section.block",
				"punctuation.section.function",
				"punctuation.section.method",
				"punctuation.section.class",
				"punctuation.section.array",
				"punctuation.separator",
				"punctuation.accessor"
			],
			"settings": {
				"foreground": "#cdcdcd"
			}
		}
	]
}